{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Bot (tsx)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--inspect",
        "./node_modules/tsx/dist/cli.mjs"
      ],
      "args": ["src/index.ts"],
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
